{
  "name": "obsidian-plugin-unocss",
  "type": "module",
  "version": "0.3.0",
  "packageManager": "pnpm@9.14.2",
  "description": "A Obsidian.md plugin that allows you to generate and apply UnoCSS and Tailwind CSS supported styles right in your notes.",
  "author": {
    "name": "Nólëbase",
    "email": "nolebase@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "keywords": [
    "nolebase",
    "readabilities",
    "markdown",
    "nolebase-integration",
    "obsidian",
    "obsidian-plugin",
    "unocss",
    "tailwindcss",
    "unocss-plugin"
  ],
  "main": "./dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "cleanup": "rm -rf ./main.js && rm -rf ./main.js.map",
    "build": "unbuild && pnpm run cleanup && pnpm run copy",
    "copy": "cp ./dist/main.js ./main.js && cp ./dist/manifest.json ./manifest.json",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@codemirror/view": "^6.35.0",
    "@types/node": "^22.9.3",
    "@unocss/core": "^0.64.1",
    "@unocss/eslint-config": "^0.64.1",
    "@unocss/eslint-plugin": "^0.64.1",
    "@unocss/reset": "^0.64.1",
    "@unocss/transformer-directives": "^0.64.1",
    "@unocss/transformer-variant-group": "^0.64.1",
    "builtin-modules": "^3.3.0",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "magic-string": "^0.30.13",
    "obsidian": "^1.7.2",
    "ofetch": "^1.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "unbuild": "^2.0.0",
    "unocss": "^0.64.1"
  }
}
