{
  "name": "obsidian-plugin-unocss",
  "type": "module",
  "version": "0.6.1",
  "packageManager": "pnpm@10.12.1",
  "description": "A Obsidian.md plugin that allows you to generate and apply UnoCSS and Tailwind CSS supported styles right in your notes.",
  "author": {
    "name": "Nólëbase",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nolebase"
  },
  "license": "MIT",
  "keywords": [
    "nolebase",
    "readabilities",
    "markdown",
    "nolebase-integration",
    "obsidian",
    "obsidian-plugin",
    "unocss",
    "tailwindcss",
    "unocss-plugin"
  ],
  "main": "./dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "typecheck": "tsc --noEmit",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@codemirror/view": "^6.37.1",
    "@types/node": "^22.15.31",
    "@unocss/core": "^66.2.0",
    "@unocss/eslint-config": "^66.2.0",
    "@unocss/eslint-plugin": "^66.2.0",
    "@unocss/preset-attributify": "^66.2.0",
    "@unocss/preset-icons": "^66.2.0",
    "@unocss/preset-uno": "^66.2.0",
    "@unocss/reset": "^66.2.0",
    "@unocss/transformer-directives": "^66.2.0",
    "@unocss/transformer-variant-group": "^66.2.0",
    "@unrteljs/eval": "^0.2.1",
    "builtin-modules": "^5.0.0",
    "bumpp": "^10.1.1",
    "eslint": "^9.28.0",
    "magic-string": "^0.30.17",
    "obsidian": "^1.8.7",
    "ofetch": "^1.4.1",
    "taze": "^19.1.0",
    "tsx": "^4.20.1",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  }
}
